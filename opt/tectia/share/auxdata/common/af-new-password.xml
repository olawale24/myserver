<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form PUBLIC "ssh-secsh-auth-form-1.dtd" "ssh-secsh-auth-form-1.dtd">
<form id="new-password">
<label/>
<label>%%%PROMPT%%%</label>
<label>Host: %%%SERVERHOST-SERVERPORT%%%</label>
<label>User: %%%USERNAME%%%</label>
<secret ref="password">
<label>New password</label>
</secret>
<secret ref="check">
<label>Again</label>
<send submission="ok" />
</secret>
<submit ref="ok" >
<label>OK</label>
</submit>
<submit ref="cancel" >
<label>Cancel</label>
</submit>
</form>
