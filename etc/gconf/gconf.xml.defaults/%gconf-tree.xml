					<entry name="candPerPage" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/candPerPage"/>
					<entry name="plainZhuyin" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/plainZhuyin"/>
					<entry name="numpadAlwaysNumber" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/numpadAlwaysNumber"/>
					<entry name="syncCapsLock" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/syncCapsLock"/>
					<entry name="forceLowercaseEnglish" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/forceLowercaseEnglish"/>
					<entry name="maxChiSymbolLen" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/maxChiSymbolLen"/>
					<entry name="escCleanAllBuf" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/escCleanAllBuf"/>
					<entry name="easySymbolInput" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/easySymbolInput"/>
					<entry name="addPhraseDirection" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/addPhraseDirection"/>
					<entry name="autoShiftCur" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/autoShiftCur"/>
					<entry name="hsuSelKeyType" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/hsuSelKeyType"/>
					<entry name="selKeys" mtime="1503048394" schema="/schemas/desktop/ibus/engine/Chewing/selKeys"/>
